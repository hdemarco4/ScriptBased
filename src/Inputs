'((PTRANS(ACTOR?SHOPPER)
            (OBJECT?SHOPPER)
            (TO?STORE))
    (PTRANS(ACTOR?SHOPPER)
            (OBJECT?BOUGHT)
            (TO?SHOPPER))
    (ATRANS(ACTOR?STORE)
            (OBJECT?BOUGHT)
            (FROM?STORE)
            (TO?SHOPPER))
    (ATRANS(ACTOR?SHOPPER)
            (OBJECT(MONEY))
            (FROM?SHOPPER)
            (TO?STORE))
    (PTRANS(ACTOR?SHOPPER)
            (OBJECT?SHOPPER)
            (FROM?STORE)
            (TO?ELSEWHERE))))
